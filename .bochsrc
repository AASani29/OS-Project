cpu: model=corei7_ivy_bridge_3770k, count=2, ips=50000000, ignore_bad_msrs=1
cpuid: x86_64=1, vmx=1, avx=1, pcid=1, apic=xapic, aes=1, mmx=1, sep=1

megs: 1024

clock: sync=realtime, time0=local

vga_update_interval: 300000
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest
private_colormap: enabled=0

ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=disk, mode=flat, path="./obj/certikos.img", cylinders=130, heads=16, spt=63
floppy_bootsig_check: disabled=0
boot: disk

log: misc/bochs.log
panic: action=fatal
error: action=report
info: action=ignore
debug: action=ignore
debug_symbols: file="./obj/sys/kernel.sym"
magic_break: enabled=1

keyboard_serial_delay: 250
keyboard_paste_delay: 100000
keyboard_mapping: enabled=0, map=
mouse: enabled=0

com1: enabled=1, mode=file, dev=log
